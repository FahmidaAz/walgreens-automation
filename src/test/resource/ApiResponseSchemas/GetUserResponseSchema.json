{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "id",
        "username",
        "firstName",
        "lastName",
        "email",
        "password",
        "phone",
        "userStatus"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                811
            ]
        },
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "fahmidaazmin"
            ]
        },
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema",
            "examples": [
                "Fahmida"
            ]
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema",
            "examples": [
                "Azmin"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "shoilyazmin@gmail.com"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "12345"
            ]
        },
        "phone": {
            "type": "string",
            "default": "",
            "title": "The phone Schema",
            "examples": [
                "6463841115"
            ]
        },
        "userStatus": {
            "type": "integer",
            "default": 0,
            "title": "The userStatus Schema",
            "examples": [
                0
            ]
        }
    },
    "examples": [{
        "id": 811,
        "username": "fahmidaazmin",
        "firstName": "Fahmida",
        "lastName": "Azmin",
        "email": "shoilyazmin@gmail.com",
        "password": "12345",
        "phone": "6463841115",
        "userStatus": 0
    }]
}